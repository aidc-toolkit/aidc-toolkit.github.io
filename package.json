{
  "name": "@aidc-toolkit/doc",
  "version": "1.0.22-alpha",
  "description": "Documentation for AIDC Toolkit",
  "type": "module",
  "private": true,
  "homepage": "https://aidc-toolkit.com/",
  "repository": "github:aidc-toolkit/doc",
  "bugs": {
    "url": "https://github.com/aidc-toolkit/doc/issues"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Kevin Dean",
    "email": "Kevin.Dean@datadevelopment.com",
    "url": "https://www.linkedin.com/in/kdean"
  },
  "scripts": {
    "lint": "eslint",
    "copy-to-public": "copy-files-from-to --config copy-to-public.json",
    "generator": "tsx src/generator.ts",
    "build:core": "npm run copy-to-public && typedoc && npm run generator && vitepress build",
    "build:dev": "rimraf site/public && npm run build:core -- $*",
    "build:doc": "npm run build:core -- $*",
    "server": "vitepress dev",
    "server-preview": "npm run build:dev && vitepress preview"
  },
  "devDependencies": {
    "@aidc-toolkit/dev": "alpha",
    "copy-files-from-to": "^3.13.0",
    "rimraf": "^6.1.2",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-plugin-mdn-links": "^5.0.10",
    "typedoc-vitepress-theme": "^1.1.2",
    "vitepress": "next"
  },
  "dependencies": {
    "@aidc-toolkit/app-extension": "alpha",
    "@aidc-toolkit/core": "alpha"
  }
}
