{
  "name": "@aidc-toolkit/doc",
  "version": "1.0.39-beta",
  "description": "Documentation for AIDC Toolkit",
  "type": "module",
  "private": true,
  "homepage": "https://aidc-toolkit.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aidc-toolkit/doc.git"
  },
  "bugs": {
    "url": "https://github.com/aidc-toolkit/doc/issues"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Kevin Dean",
    "email": "Kevin.Dean@datadevelopment.com",
    "url": "https://www.linkedin.com/in/kdean"
  },
  "scripts": {
    "lint": "eslint",
    "typedoc": "typedoc && replace --silent 'https://aidc-toolkit\\.com/' '/' site/api --include index.md --recursive",
    "generate-app-extension-documentation": "tsx src/generator.ts",
    "build:base": "npm run typedoc && npm run generate-app-extension-documentation && vitepress build",
    "build:alpha": "tsc --noEmit && npm run build:base -- $*",
    "build:doc": "npm run build:base -- $*",
    "server": "vitepress dev --host",
    "server-preview": "npm run build:doc && vitepress preview"
  },
  "devDependencies": {
    "@aidc-toolkit/app-extension": "1.0.39-beta",
    "@aidc-toolkit/core": "1.0.36-beta",
    "@aidc-toolkit/dev": "1.0.35-beta",
    "mermaid": "^11.12.2",
    "replace": "^1.2.2",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-plugin-mdn-links": "^5.1.0",
    "typedoc-vitepress-theme": "^1.1.2",
    "vite-plugin-static-copy": "^3.1.5",
    "vitepress": "next",
    "vitepress-plugin-mermaid": "^2.0.17"
  }
}
